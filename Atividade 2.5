#include <math.h>

int main(void) {
    int C = 100;
    int J = 499;
    float M = 1;
    int JJ = 1 + J;
    printf("1 somado com %d sera %d", J, JJ);
    float JJJ = JJ / 100;
    printf(", %d dividido por 100 chegara ao resultado %f", JJ, JJJ);
    int JM = pow(JJJ, M);
    printf("%f elevado ao %f o resultado sera %d", JJJ, M, JM);
    int CM = C * JM;
    printf(", e chegando ao final o resultado sera %d por %d que ira da %d", C, JM, CM);
    
    return 0;
}
