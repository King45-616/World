#include <math.h>

int main(void) {
    int C = 100;
    int J= 500;
    int M= 12;
    int JJ = 1 + J;
    printf("1 somado com %d sera %d", J, JJ);
    int JJJ = JJ / 100;
    printf(", %d dividido por 100 chegara ao resultado %d", JJ, JJJ);
    int JM = pow(JJJ, M);
    printf("%d elevado ao %d o resultado sera %d", JJJ, M, JM);
    int CM = C * M;
    printf("e chegando ao final o resultado sera %d por %d que ira da %d", C, M, CM);

    return 0;

}
